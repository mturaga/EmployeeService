{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AppSettings": {
    "DefaultPageSize": 25,
    "MaxPageSize": 100,
    "DomainUrls": [
      {

        "name": "IdpUrl",
        "url": "http://dev.api.com/",
        "handShakeKey": "12345"
      }
    ]
  },
  
    "Serilog": {
      "MinimumLevel": "Debug",
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
          }
        }
      ]
    },
  
  "AllowedHosts": "*"
}
